@import 'partial/variables'
@import 'partial/reset'
html
  -webkit-overflow-scrolling touch
  -webkit-text-size-adjust 100%
body
  margin 0
  padding 0
  max-width 10rem
  margin 0 auto
  position relative
  word-break break-all
  font-size size-font
  line-height 1.5
  font-family family
  background color-bg
  color color-font            
                                
@font-face 
  font-family 'cn_font';
  src url('//at.alicdn.com/t/font_1471420483_5685318.eot')
